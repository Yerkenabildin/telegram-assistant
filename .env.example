# Telegram API credentials (get from https://my.telegram.org)
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Telegram usernames/IDs
PERSONAL_TG_LOGIN=your_personal_telegram_username
WORK_TG_LOGIN=your_work_telegram_username

# Optional: Emoji status ID that disables auto-reply (default: 5810051751654460532)
# AVAILABLE_EMOJI_ID=5810051751654460532

# Optional: Secret key for Quart session encryption (will be auto-generated if not set)
# SECRET_KEY=your_secret_key_here
